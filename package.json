{
  "name": "com.monitorprocesos.qa",
  "displayName": "Monitor Procesos",
  "version": "1.0.0",
  "description": "Monitor de Procesos",
  "main": "index.js",
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "ternium",
  "license": "Apache-2.0",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ie": "ng serve -o -c es5",
    "build": "ng build",
    "gen-prod": "ng build --base-href '/MonitorProcesosWeb/' --prod --build-optimizer=false",
    "gen-prod-op": "ng build --base-href '/MonitorProcesosWeb/' --prod --build-optimizer=true",
    "analizar": "ng build --prod --stats-json && webpack-bundle-analyzer dist/Monitor/stats-es2015.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "tslint": "tslint 'src/**/*.ts'"
  },
  "private": true,
  "dependencies": {
